{
  "name": "personal-document-generator",
  "version": "1.0.4",
  "description": "A simple DNI (spanish ID document number) and NIE (id number for foreing people in spain) generators.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint .",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier . --write",
    "format": "npm-run-all format:*",
    "lint": "run-s eslint prettier",
    "ncu": "ncu -i --format group",
    "prettier": "prettier . --check",
    "release:dry": "release-it --dry-run -c ./.release/release-it.json",
    "release": "release-it -c ./.release/release-it.json",
    "test": "vitest"
  },
  "keywords": [
    "dni",
    "nif",
    "nie",
    "generator"
  ],
  "author": "JuanMa Ruiz",
  "license": "MIT",
  "devDependencies": {
    "@release-it/conventional-changelog": "10.0.2",
    "coveralls": "^3.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "10.1.8",
    "npm-check-updates": "19.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.2",
    "release-it": "19.0.6",
    "vitest": "4.0.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JuanMaRuiz/document-generator.git"
  },
  "homepage": "https://github.com/JuanMaRuiz/document-generator"
}
